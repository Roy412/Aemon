{
  "name": "aemon",
  "version": "1.0.0",
  "engines": {
    "node": "10.14.x"
  },
  "description": "PBG UI Library with Storybook",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ignore-styles --require babel-core/register -r ./test/config/index.js --recursive --ui bdd ./test",
    "test:cover": "nyc --reporter=lcov mocha -r ignore-styles --require babel-core/register -r ./test/config/enzyme.js --recursive --ui bdd ./test",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o dist",
    "heroku-prebuild": "build-storybook -c .storybook -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paybygroup/aemon.git"
  },
