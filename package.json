{
  "name": "aemon",
  "version": "1.1.0",
  "engines": {
    "node": "10.14.x",
    "npm": "6.x"
  },
  "description": "PBG UI Library with Storybook",
  "main": "./dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "lint": "node scripts/lint.js",
    "format:stage": "precise-commits",
    "test": "mocha ./test/config/index.js ./test",
    "test:watch": "mocha ./test/config/index.js --watch",
    "test:cover": "nyc --reporter=lcov mocha -r ignore-styles --compilers js:@babel/register -r ./test/config/enzyme.js --recursive --ui bdd ./test",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "heroku-postbuild": "build-storybook -c .storybook -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paybygroup/aemon.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paybygroup/aemon/issues"
  },
  "homepage": "https://github.com/paybygroup/aemon#readme",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "npm": "^6.5.0",
    "prop-types": "^15.6.2",
    "react": "~16.3.0",
    "react-dom": "~16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16.3": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
