    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.17.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "storybook-addon-figma": "^0.1.0",
    "style-loader": "^0.23.1",
