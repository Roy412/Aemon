{
  "name": "aemon",
  "version": "1.0.0",
  "engines": {
    "node": "10.14.x",
    "npm": "6.x"
  },
  "description": "PBG UI Library with Storybook",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ignore-styles --compilers js:@babel/register -r ./test/config/index.js --recursive --ui bdd ./test",
    "test:cover": "nyc --reporter=lcov mocha -r ignore-styles --compilers js:@babel/register -r ./test/config/enzyme.js --recursive --ui bdd ./test",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o dist",
    "heroku-postbuild": "build-storybook -c .storybook -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paybygroup/aemon.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paybygroup/aemon/issues"
  },
  "homepage": "https://github.com/paybygroup/aemon#readme",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "prop-types": "^15.6.2",
    "react": "~16.3.0",
    "react-dom": "~16.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16.3": "^1.4.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0",
    "node-sass": "^4.10.0",
    "nyc": "^13.1.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1"
  }
